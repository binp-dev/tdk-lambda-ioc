record(stringin, "PS$(UNIT_ADR):ser_numb") {
	field(SCAN, "Passive")
	field(DTYP, "stream")
	field(INP, "@tdk_lambda.proto getSerialNumber($(UNIT_ADR)) PS1")
}

#====================================

record(ai, "PS$(UNIT_ADR):volt_real") {
	field(SCAN, "1 second")
	field(DTYP, "stream")
	field(INP, "@tdk_lambda.proto getVoltageAndCurrent($(UNIT_ADR)) PS1")
	field(EGU, "V")
}

record(ai, "PS$(UNIT_ADR):curr_real") {
	field(SCAN, "Passive")
	field(EGU, "A")
}


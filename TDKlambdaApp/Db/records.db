	field(EGU, "V")
}

record(ao, "PS$(UNIT_ADR):under_volt_set_point") {
	field(DTYP, "stream")
	field(OUT, "@tdk_lambda.proto setUnderVoltageLimit($(UNIT_ADR)) PS1")
	field(EGU, "V")
}

